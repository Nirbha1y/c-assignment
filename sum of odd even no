#include <stdio.h>
void sume(int num){
    int odd = 0 , even = 0,i,j;
    printf("Enter a number : ");
    scanf("%d",&num);
    for(i=1;i<=num;i+=2){
        odd+=i;
    }
    printf("Sum of odd number : %d\n",odd);

    int num2;
    printf("Enter a number : ");
    scanf("%d",&num2);
    for(j=2;j<=num2;j+=2){
        even +=j;

    }
    printf("Sum of even number : %d\n",even);

}
